{
  "extends": [
    "react-app",
    "eslint:recommended"
  ],
  "plugins": [
    "jest"
  ],
  "rules": {
    "no-console": "warn",
    "no-alert": "warn",
    "react/self-closing-comp": "error",
    "@typescript-eslint/explicit-function-return-type": "off",
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "warn",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}
